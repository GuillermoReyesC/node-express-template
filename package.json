{
  "name": "node-express-template",
  "version": "1.0.1",
  "main": "src/server.js",
  "license": "MIT",
  "scripts": {
    "start": "cross-env  NODE_PATH=. NODE_ENV=production node src/server.js",
    "dev": "cross-env NODE_PATH=. nodemon src/server.js",
    "migrate": "npx sequelize-cli db:migrate",
    "seeds": "npx sequelize-cli db:seed:all",
    "migrate:seeds:all": "yarn migrate && yarn seeds",
    "test": "NODE_PATH=. jest --runInBand --coverage --forceExit"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "body-parser": "^1.20.0",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "pg": "^8.7.3",
    "react-scripts": "^5.0.1",
    "sequelize": "^6.21.2",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "jest": "^28.1.2",
    "nodemon": "^2.0.18",
    "sequelize-cli": "^6.4.1",
    "sqlite3": "^5.0.8",
    "supertest": "^6.2.4"
  },
  "description": "To make it easy for you to get started with GitLab, here's a list of recommended next steps.",
  "directories": {
    "test": "test"
  },
